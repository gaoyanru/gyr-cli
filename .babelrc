{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["> 5%", "last 2 versions"]
        },
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }],
    "syntax-dynamic-import",
    "transform-runtime"
  ]
}
